{
	// Place your x2 workspace snippets here. Each snippet is defined under a snippet name and has a scope, prefix, body and 
	// description. Add comma separated ids of the languages where the snippet is applicable in the scope field. If scope 
	// is left empty or omitted, the snippet gets applied to all languages. The prefix is what is 
	// used to trigger the snippet and the body will be expanded and inserted. Possible variables are: 
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. 
	// Placeholders with the same ids are connected.
	// Example:
	// "Print to console": {
	// 	"scope": "javascript,typescript",
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"anewbehaviour": {
		"scope": "javascript,typescript",
		"prefix": "ffbehaviour",
		"body": [
			"describe(\"${1:somethin}\", ()=>{",
			"\tit(\"should ${2:be}\",() => {",
			"\t\tconst result = ${3:null};",
			"\t\texpect(result).to${0}",
			"\t});",
			"});"
		],
		"description": "a new behaviour"
	},
	"anewtest": {
		"scope": "javascript,typescript",
		"prefix": "fftest",
		"body": [
			"it(\"should ${1:be}\",() => {",
			"\tconst result = ${2:null};",
			"\texpect(result).to${0}",
			"});"
		],
		"description": "a new test"
	},
	"adescriptionof": {
		"scope": "javascript,typescript",
		"prefix": "ffdescription",
		"body": [
			"describe(\"${1:thing}\",() => {",
			"\t${0}",
			"});"
		],
		"description": "a description"
	}
}